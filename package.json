{
	"name": "missionhub",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:snapshot": "jest -u",
		"test:changes": "jest -o",
		"test:w": "npm run test:watch",
		"test:c": "npm run test:coverage",
		"test:s": "npm run test:snapshot",
		"ios": "react-native run-ios",
		"lint": "eslint ./src ./__tests__",
		"lint:fix": "eslint ./src ./__tests__ --fix",
		"android": "react-native run-android",
		"android:install:dev": "adb install android/app/build/outputs/apk/app-debug.apk",
		"android:install:release": "adb install android/app/build/outputs/apk/app-release.apk",
		"android:build": "react-native run-android --variant=release",
		"onesky:upload": "babel-node ./bin/oneskyUpload.js",
		"onesky:download": "babel-node ./bin/oneskyDownload.js"
	},
	"dependencies": {
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"base64-url": "^2.2.0",
		"buffer": "^5.1.0",
		"color": "^3.0.0",
		"core-js": "^2.5.5",
		"i18next": "^11.2.2",
		"js-sha256": "^0.9.0",
		"jsan": "^3.1.9",
		"jsonapi-datastore": "^0.4.0-beta",
		"lodash": "^4.17.5",
		"moment": "^2.22.1",
		"prop-types": "^15.6.1",
		"qs": "^6.5.1",
		"random-string": "^0.2.0",
		"react": "^16.3.2",
		"react-i18next": "^7.6.0",
		"react-native": "^0.55.2",
		"react-native-animatable": "^1.2.4",
		"react-native-config": "^0.11.5",
		"react-native-default-preference": "^1.3.1",
		"react-native-device-info": "^0.21.5",
		"react-native-fabric": "^0.5.1",
		"react-native-fbsdk": "^0.7.0",
		"react-native-omniture": "https://github.com/dsgoers/react-native-omniture#4eb498b88f18942b3fdb03094f99b2d77fe8fc15",
		"react-native-parallax-scroll-view": "^0.21.0",
		"react-native-push-notification": "https://github.com/CruGlobal/react-native-push-notification",
		"react-native-scrollable-tab-view": "^0.8.0",
		"react-native-snap-carousel": "^3.7.0",
		"react-native-vector-icons": "^4.6.0",
		"react-navigation": "^1.5.11",
		"react-navigation-redux-helpers": "^1.0.5",
		"react-redux": "^5.0.7",
		"reactotron-react-native": "^1.14.0",
		"reactotron-redux": "^1.13.0",
		"redux": "^4.0.0",
		"redux-persist": "^5.9.1",
		"redux-thunk": "^2.2.0",
		"reselect": "^3.0.1",
		"uuid": "^3.2.1"
	},
	"devDependencies": {
		"@brainly/onesky-utils": "^1.2.0",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.2.3",
		"babel-jest": "^22.4.3",
		"babel-preset-react-native": "^4.0.0",
		"dotenv": "^5.0.1",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"enzyme-to-json": "^3.3.3",
		"eslint": "^4.19.1",
		"eslint-plugin-import": "^2.11.0",
		"eslint-plugin-react": "^7.7.0",
		"jest": "^22.4.3",
		"jest-cli": "^22.4.3",
		"mockdate": "^2.0.2",
		"react-dom": "^16.3.2",
		"react-test-renderer": "^16.3.2",
		"redux-mock-store": "^1.5.1"
	},
	"jest": {
		"preset": "react-native",
		"setupFiles": [
			"<rootDir>/__mock__/react-native.js",
			"<rootDir>/__mock__/react-native-device-info.js",
			"<rootDir>/__mock__/react-native-fabric.js"
		],
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"transformIgnorePatterns": [
			"node_modules/(?!(jest-)?react-native|react-navigation)"
		],
		"coverageDirectory": "./coverage/",
		"collectCoverageFrom": [
			"src/**/*.js"
		]
	},
	"rnpm": {
		"assets": [
			"./assets/fonts"
		]
	}
}
