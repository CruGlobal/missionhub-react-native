{
  "name": "missionhub",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:snapshot": "jest -u",
    "test:changes": "jest -o",
    "test:w": "npm run test:watch",
    "test:c": "npm run test:coverage",
    "test:s": "npm run test:snapshot",
    "ios": "react-native run-ios",
    "lint": "eslint ./*.js ./src ./__tests__",
    "lint:fix": "eslint ./*.js ./src ./__tests__ --fix",
    "android": "react-native run-android",
    "android:install:dev": "adb install android/app/build/outputs/apk/app-debug.apk",
    "android:install:release": "adb install android/app/build/outputs/apk/app-release.apk",
    "android:build": "react-native run-android --variant=release",
    "codepush-ios-staging": "appcenter codepush release-react -a Cru/MissionHub-iOS --plist-file ios/MissionHub/Info.plist",
    "codepush-android-staging": "appcenter codepush release-react -a Cru/MissionHub-Android",
    "onesky:upload": "babel-node ./bin/oneskyUpload.js",
    "onesky:download": "babel-node ./bin/oneskyDownload.js",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "@ringierag/snowplow-reactjs-native-tracker": "^0.0.3",
    "base64-url": "^2.2.0",
    "buffer": "^5.2.1",
    "color": "^3.1.0",
    "core-js": "^2.5.7",
    "i18next": "^11.9.1",
    "js-sha256": "^0.9.0",
    "jsan": "^3.1.9",
    "jsonapi-datastore": "^0.4.0-beta",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "qs": "^6.5.1",
    "random-string": "^0.2.0",
    "react": "16.6.0-alpha.8af6728",
    "react-i18next": "^8.0.7",
    "react-native": "^0.57.3",
    "react-native-animatable": "^1.3.0",
    "react-native-code-push": "^5.4.2",
    "react-native-config": "^0.11.5",
    "react-native-default-preference": "^1.3.2",
    "react-native-device-info": "^0.24.0",
    "react-native-fabric": "^0.5.2",
    "react-native-fbsdk": "^0.8.0",
    "react-native-omniture": "https://github.com/CruGlobal/react-native-omniture",
    "react-native-parallax-scroll-view": "^0.21.3",
    "react-native-push-notification": "https://github.com/CruGlobal/react-native-push-notification",
    "react-native-scrollable-tab-view": "^0.8.0",
    "react-native-snap-carousel": "^3.7.5",
    "react-native-vector-icons": "^6.0.2",
    "react-native-view-overflow": "^0.0.3",
    "react-navigation": "^2.18.0",
    "react-navigation-redux-helpers": "^2.0.6",
    "react-redux": "^5.0.7",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "stacktrace-js": "^2.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@brainly/onesky-utils": "^1.3.1",
    "appcenter-cli": "^1.1.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "dotenv": "^6.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "metro-react-native-babel-preset": "^0.48.1",
    "mockdate": "^2.0.2",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "react-dom": "^16.5.2",
    "react-test-renderer": "16.6.0-alpha.8af6728",
    "redux-mock-store": "^1.5.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/__mock__/react-native.js",
      "<rootDir>/__mock__/react-native-device-info.js",
      "<rootDir>/__mock__/react-native-fabric.js",
      "<rootDir>/__mock__/react-native-code-push.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
