{
	"name": "missionhub",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:snapshot": "jest -u",
		"test:changes": "jest -o",
		"test:w": "npm run test:watch",
		"test:c": "npm run test:coverage",
		"test:s": "npm run test:snapshot",
		"ios": "react-native run-ios",
		"lint": "eslint ./src ./__tests__",
		"lint:fix": "eslint ./src ./__tests__ --fix",
		"android": "react-native run-android",
		"android:install:dev": "adb install android/app/build/outputs/apk/app-debug.apk",
		"android:install:release": "adb install android/app/build/outputs/apk/app-release.apk",
		"android:build": "react-native run-android --variant=release",
		"onesky:upload": "babel-node ./bin/oneskyUpload.js",
		"onesky:download": "babel-node ./bin/oneskyDownload.js"
	},
	"dependencies": {
		"axios": "0.17.0",
		"babel-plugin-transform-decorators-legacy": "1.3.4",
		"base64-url": "2.2.0",
		"buffer": "5.1.0",
		"color": "2.0.0",
		"i18next": "10.2.1",
		"js-sha256": "0.9.0",
		"jsan": "3.1.9",
		"jsonapi-datastore": "0.4.0-beta",
		"lodash": "4.17.4",
		"moment": "2.19.1",
		"prop-types": "15.6.0",
		"qs": "6.5.1",
		"random-string": "0.2.0",
		"react": "16.0.0",
		"react-i18next": "7.1.1",
		"react-native": "0.50.4",
		"react-native-animatable": "1.2.4",
		"react-native-default-preference": "1.3.1",
		"react-native-device-info": "0.12.1",
		"react-native-fabric": "0.5.1",
		"react-native-fbsdk": "0.7.0",
		"react-native-omniture": "https://github.com/dsgoers/react-native-omniture#4eb498b88f18942b3fdb03094f99b2d77fe8fc15",
		"react-native-push-notification": "3.0.2",
		"react-native-scrollable-tab-view": "0.8.0",
		"react-native-snap-carousel": "3.4.0",
		"react-native-vector-icons": "4.4.2",
		"react-navigation": "1.1.2",
		"react-navigation-redux-helpers": "1.0.1",
		"react-redux": "5.0.6",
		"reactotron-react-native": "1.12.3",
		"reactotron-redux": "1.12.2",
		"redux": "3.7.2",
		"redux-persist": "4.10.1",
		"redux-thunk": "2.2.0",
		"reselect": "3.0.1",
		"uuid": "3.1.0"
	},
	"devDependencies": {
		"babel-cli": "6.26.0",
		"babel-core": "6.26.0",
		"babel-eslint": "8.0.1",
		"babel-jest": "21.2.0",
		"babel-preset-react-native": "4.0.0",
		"dotenv": "4.0.0",
		"enzyme": "3.2.0",
		"enzyme-adapter-react-16": "1.1.0",
		"enzyme-to-json": "3.3.0",
		"eslint": "4.10.0",
		"eslint-plugin-import": "2.8.0",
		"eslint-plugin-react": "7.4.0",
		"jest": "21.2.1",
		"jest-cli": "21.2.1",
		"mockdate": "2.0.2",
		"onesky-utils": "1.2.0",
		"react-dom": "16.2.0",
		"react-test-renderer": "16.0.0-beta.5",
		"redux-mock-store": "1.3.0"
	},
	"jest": {
		"preset": "react-native",
		"setupFiles": [
			"<rootDir>/__mock__/react-native-device-info.js"
		],
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"transformIgnorePatterns": [
			"node_modules/(?!(jest-)?react-native|react-navigation)"
		],
		"coverageDirectory": "./coverage/",
		"collectCoverageFrom": [
			"src/**/*.js"
		]
	},
	"rnpm": {
		"assets": [
			"./assets/fonts"
		]
	}
}
