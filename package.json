{
  "name": "missionhub",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "open": "open ios/MissionHub.xcworkspace/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:snapshot": "jest -u",
    "test:changes": "jest -o",
    "test:w": "npm run test:watch",
    "test:c": "npm run test:coverage",
    "test:s": "npm run test:snapshot",
    "ios": "react-native run-ios",
    "ios:pod": "cd ios && pod install",
    "lint": "eslint './*.{js,ts,tsx}' './src/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint './*.{js,ts,tsx}' './src/**/*.{js,ts,tsx}' --fix",
    "lint:ts": "tsc",
    "prettier:check": "prettier '{{src,bin,testUtils,__{tests,mocks}__}/**/*.{js,json,ts,tsx},./*.{js,json,ts,tsx,yml}}' --list-different",
    "prettier:write": "prettier '{{src,bin,testUtils,__{tests,mocks}__}/**/*.{js,json,ts,tsx},./*.{js,json,ts,tsx,yml}}' --write",
    "android": "react-native run-android",
    "android:install:dev": "adb install android/app/build/outputs/apk/app-debug.apk",
    "android:install:release": "adb install android/app/build/outputs/apk/app-release.apk",
    "android:build": "react-native run-android --variant=release",
    "gql:codegen": "apollo client:codegen --target typescript",
    "gql:codegen:w": "apollo client:codegen --target typescript --watch",
    "codepush-ios-staging": "appcenter codepush release-react -a Cru/MissionHub-iOS --plist-file ios/MissionHub/Info.plist",
    "codepush-android-staging": "appcenter codepush release-react -a Cru/MissionHub-Android",
    "onesky:upload": "babel-node ./bin/oneskyUpload.js",
    "onesky:download": "babel-node ./bin/oneskyDownload.js",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "@ringierag/snowplow-reactjs-native-tracker": "^0.0.4",
    "apollo-boost": "^0.3.1",
    "apollo-cache-persist": "^0.1.1",
    "base64-url": "^2.2.1",
    "buffer": "^5.2.1",
    "color": "^3.1.1",
    "core-js": "^3.1.2",
    "graphql": "^14.1.1",
    "i18next": "^15.1.3",
    "js-sha256": "^0.9.0",
    "jsan": "^3.1.13",
    "jsonapi-datastore": "^0.4.0-beta",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "qs": "^6.7.0",
    "random-string": "^0.2.0",
    "react": "^16.8.6",
    "react-apollo-hooks": "^0.4.5",
    "react-i18next": "^10.10.0",
    "react-native": "^0.59.8",
    "react-native-animatable": "^1.3.2",
    "react-native-code-push": "^5.6.0",
    "react-native-config": "^0.11.7",
    "react-native-default-preference": "https://github.com/kevinresol/react-native-default-preference#46697032198861affe73f4bf9d438c328cc312b2",
    "react-native-device-info": "^2.0.2",
    "react-native-fbsdk": "^0.8.0",
    "react-native-firebase": "^5.3.1",
    "react-native-gesture-handler": "^1.2.1",
    "react-native-image-picker": "^0.28.1",
    "react-native-markdown-renderer": "^3.2.8",
    "react-native-omniture": "https://github.com/CruGlobal/react-native-omniture",
    "react-native-parallax-scroll-view": "https://github.com/nicotroia/react-native-parallax-scroll-view.git#fix/sticky-header-position-when-hidden",
    "react-native-push-notification": "https://github.com/CruGlobal/react-native-push-notification",
    "react-native-scrollable-tab-view": "^0.10.0",
    "react-native-snap-carousel": "^3.8.0",
    "react-native-vector-icons": "^6.4.2",
    "react-native-view-overflow": "^0.0.4",
    "react-navigation": "3.11.0",
    "react-navigation-hooks": "https://github.com/react-navigation/react-navigation-hooks#master",
    "react-navigation-redux-helpers": "^3.0.2",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rollbar-react-native": "^0.5.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@brainly/onesky-utils": "^1.3.1",
    "@types/color": "^3.0.0",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.18",
    "@types/react-native": "^0.57.60",
    "@types/react-redux": "^7.0.9",
    "@types/react-test-renderer": "^16.8.1",
    "@types/redux-mock-store": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "apollo": "^2.9.0",
    "appcenter-cli": "^1.1.18",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "dotenv": "^8.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "mockdate": "^2.0.2",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "react-dom": "^16.8.6",
    "react-hooks-testing-library": "^0.5.0",
    "react-native-testing-library": "^1.7.0",
    "react-test-renderer": "^16.8.6",
    "redux-mock-store": "^1.5.3",
    "typescript": "^3.4.5"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/__mock__/react-native-device-info.js",
      "<rootDir>/__mock__/react-native-code-push.js",
      "<rootDir>/__mock__/react-native-firebase.js",
      "<rootDir>/__mock__/react-native-gesture-handler.js",
      "<rootDir>/__mock__/rollbar-react-native.js",
      "<rootDir>/__mock__/react-native-omniture.js",
      "<rootDir>/__mock__/init-i18next.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mock__/hideWarnings.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@?react-navigation|react-navigation-redux-helpers)"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.{js,ts,tsx}"
    ],
    "clearMocks": true
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
