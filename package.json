{
  "name": "missionhub",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "open": "open ios/MissionHub.xcworkspace/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:snapshot": "jest -u",
    "test:changes": "jest -o",
    "test:w": "npm run test:watch",
    "test:c": "npm run test:coverage",
    "test:s": "npm run test:snapshot",
    "ios": "react-native run-ios",
    "ios:pod": "cd ios && pod install",
    "lint": "eslint ./*.js ./src",
    "lint:fix": "eslint ./*.js ./src --fix",
    "prettier:check": "prettier '{{src,bin,testUtils,__{tests,mocks}__}/**/*.{js,json},./*.{js,json,yml}}' --list-different",
    "prettier:write": "prettier '{{src,bin,testUtils,__{tests,mocks}__}/**/*.{js,json},./*.{js,json,yml}}' --write",
    "android": "react-native run-android",
    "android:install:dev": "adb install android/app/build/outputs/apk/app-debug.apk",
    "android:install:release": "adb install android/app/build/outputs/apk/app-release.apk",
    "android:build": "react-native run-android --variant=release",
    "android:bundleReleaseAssets": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "codepush-ios-staging": "appcenter codepush release-react -a Cru/MissionHub-iOS --plist-file ios/MissionHub/Info.plist",
    "codepush-android-staging": "appcenter codepush release-react -a Cru/MissionHub-Android",
    "onesky:upload": "babel-node ./bin/oneskyUpload.js",
    "onesky:download": "babel-node ./bin/oneskyDownload.js",
    "precommit": "pretty-quick --staged"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@ringierag/snowplow-reactjs-native-tracker": "^0.0.3",
    "apollo-boost": "^0.1.27",
    "apollo-cache-persist": "^0.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "base64-url": "^2.2.1",
    "buffer": "^5.2.1",
    "color": "^3.1.0",
    "core-js": "^3.0.0",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "i18next": "^15.0.9",
    "js-sha256": "^0.9.0",
    "jsan": "^3.1.13",
    "jsonapi-datastore": "^0.4.0-beta",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.6.1",
    "qs": "^6.7.0",
    "random-string": "^0.2.0",
    "react": "^16.3.2",
    "react-apollo": "^2.3.3",
    "react-i18next": "^7.6.0",
    "react-native": "^0.55.2",
    "react-native-animatable": "^1.2.4",
    "react-native-code-push": "^5.5.2",
    "react-native-config": "^0.11.5",
    "react-native-default-preference": "^1.3.1",
    "react-native-device-info": "^0.24.3",
    "react-native-fbsdk": "^0.8.0",
    "react-native-firebase": "^5.2.1",
    "react-native-image-picker": "^0.27.1",
    "react-native-markdown-renderer": "^3.2.8",
    "react-native-omniture": "https://github.com/CruGlobal/react-native-omniture",
    "react-native-parallax-scroll-view": "https://github.com/nicotroia/react-native-parallax-scroll-view.git#fix/sticky-header-position-when-hidden",
    "react-native-push-notification": "https://github.com/CruGlobal/react-native-push-notification",
    "react-native-scrollable-tab-view": "^0.8.0",
    "react-native-snap-carousel": "^3.7.0",
    "react-native-vector-icons": "^6.1.0",
    "react-native-view-overflow": "^0.0.3",
    "react-navigation": "2.18.3",
    "react-navigation-redux-helpers": "^2.0.5",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^4.0.0",
    "rollbar-react-native": "^0.3.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@brainly/onesky-utils": "^1.3.1",
    "appcenter-cli": "^1.1.14",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.4",
    "babel-preset-react-native": "^4.0.0",
    "dotenv": "^7.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "mockdate": "^2.0.2",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "react-dom": "^16.3.2",
    "react-test-renderer": "^16.3.2",
    "redux-mock-store": "^1.5.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/__mock__/react-native.js",
      "<rootDir>/__mock__/react-native-device-info.js",
      "<rootDir>/__mock__/react-native-code-push.js",
      "<rootDir>/__mock__/react-native-firebase.js",
      "<rootDir>/__mock__/rollbar-react-native.js",
      "<rootDir>/__mock__/react-native-omniture.js",
      "<rootDir>/__mock__/init-i18next.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation)"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "clearMocks": true
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
