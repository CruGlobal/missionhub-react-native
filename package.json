{
  "name": "missionhub",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "open": "open ios/MissionHub.xcworkspace/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:snapshot": "jest -u",
    "test:changes": "jest -o",
    "test:w": "npm run test:watch",
    "test:c": "npm run test:coverage",
    "test:s": "npm run test:snapshot",
    "ios": "react-native run-ios",
    "ios:pod": "cd ios && pod install",
    "lint": "eslint './*.{js,ts,tsx}' './{src,testUtils}/**/*.{js,ts,tsx}'",
    "lint:fix": "eslint './*.{js,ts,tsx}' './{src,testUtils}/**/*.{js,ts,tsx}' --fix",
    "lint:ts": "tsc",
    "prettier:check": "prettier '{{src,bin,testUtils,__{tests,mocks}__}/**/*.{js,json,ts,tsx},./*.{js,json,ts,tsx,yml}}' --list-different",
    "prettier:write": "prettier '{{src,bin,testUtils,__{tests,mocks}__}/**/*.{js,json,ts,tsx},./*.{js,json,ts,tsx,yml}}' --write",
    "android": "react-native run-android",
    "android:install:dev": "adb install android/app/build/outputs/apk/app-debug.apk",
    "android:install:release": "adb install android/app/build/outputs/apk/app-release.apk",
    "android:build": "react-native run-android --variant=release",
    "gql:codegen": "apollo client:codegen --target typescript",
    "gql:codegen:w": "apollo client:codegen --target typescript --watch",
    "gql:schema": "apollo client:download-schema",
    "codepush-ios-staging": "appcenter codepush release-react -a Cru/MissionHub-iOS --plist-file ios/MissionHub/Info.plist",
    "codepush-android-staging": "appcenter codepush release-react -a Cru/MissionHub-Android",
    "onesky:upload": "babel-node ./bin/oneskyUpload.js",
    "onesky:download": "babel-node ./bin/oneskyDownload.js",
    "precommit": "pretty-quick --staged",
    "postinstall": "jetify"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@babel/runtime": "^7.6.3",
    "@react-navigation/core": "3.4.1",
    "@react-navigation/native": "^3.5.0",
    "@ringierag/snowplow-reactjs-native-tracker": "^0.0.4",
    "@types/qs": "^6.5.3",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-cache-persist": "^0.1.1",
    "apollo-link": "^1.2.12",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "base64-url": "^2.3.2",
    "buffer": "^5.4.0",
    "color": "^3.1.2",
    "core-js": "^3.2.1",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "i18next": "^17.0.11",
    "js-sha256": "^0.9.0",
    "jsan": "^3.1.13",
    "jsonapi-datastore": "^0.4.0-beta",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "qs": "^6.7.0",
    "random-string": "^0.2.0",
    "react": "^16.10.2",
    "react-i18next": "^10.12.2",
    "react-native": "^0.61.2",
    "react-native-animatable": "^1.3.2",
    "react-native-code-push": "^5.6.1",
    "react-native-config": "^0.11.7",
    "react-native-default-preference": "^1.4.0",
    "react-native-device-info": "^2.3.2",
    "react-native-fbsdk": "^1.0.1",
    "react-native-firebase": "^5.5.6",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-image-crop-picker": "^0.25.0",
    "react-native-markdown-renderer": "^3.2.8",
    "react-native-material-menu": "^1.0.0",
    "react-native-omniture": "https://github.com/CruGlobal/react-native-omniture",
    "react-native-parallax-scroll-view": "https://github.com/nicotroia/react-native-parallax-scroll-view.git#fix/sticky-header-position-when-hidden",
    "react-native-push-notification": "https://github.com/CruGlobal/react-native-push-notification",
    "react-native-scrollable-tab-view": "^0.10.0",
    "react-native-snap-carousel": "^3.8.2",
    "react-native-vector-icons": "^6.6.0",
    "react-native-view-overflow": "^0.0.4",
    "react-navigation": "3.11.1",
    "react-navigation-hooks": "^1.0.2-alpha.0",
    "react-navigation-redux-helpers": "^3.0.2",
    "react-redux": "^5.1.1",
    "redux": "^4.0.4",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rollbar-react-native": "^0.5.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@brainly/onesky-utils": "^1.3.1",
    "@testing-library/react-hooks": "^3.1.0",
    "@types/color": "^3.0.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/faker": "^4.1.5",
    "@types/graphql": "^14.2.3",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.137",
    "@types/mockdate": "^2.0.0",
    "@types/react": "^16.9.9",
    "@types/react-native": "^0.60.21",
    "@types/react-native-snap-carousel": "^3.7.4",
    "@types/react-native-vector-icons": "^6.4.1",
    "@types/react-redux": "^7.1.2",
    "@types/react-test-renderer": "^16.9.1",
    "@types/redux-mock-store": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "apollo": "^2.18.1",
    "apollo-client": "^2.6.4",
    "apollo-link-schema": "^1.2.3",
    "appcenter-cli": "^2.1.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "dotenv": "^8.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "faker": "^4.1.0",
    "fraql": "^1.2.1",
    "graphql-tools": "^4.0.5",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "mockdate": "^2.0.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react-dom": "^16.10.2",
    "react-native-testing-library": "^1.11.1",
    "react-test-renderer": "^16.10.2",
    "redux-mock-store": "^1.5.3",
    "snapshot-diff": "^0.5.2",
    "typescript": "^3.6.4"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/__mock__/react-native.ts",
      "<rootDir>/__mock__/react-native-device-info.js",
      "<rootDir>/__mock__/react-native-code-push.js",
      "<rootDir>/__mock__/react-native-firebase.js",
      "<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js",
      "<rootDir>/__mock__/rollbar-react-native.js",
      "<rootDir>/__mock__/react-native-omniture.js",
      "<rootDir>/__mock__/init-i18next.js",
      "<rootDir>/__mock__/apolloClient.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mock__/hideWarnings.ts",
      "<rootDir>/__mock__/resetGlobalMockSeeds.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@?react-navigation|react-navigation-redux-helpers)"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.{js,ts,tsx}"
    ],
    "clearMocks": true
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
